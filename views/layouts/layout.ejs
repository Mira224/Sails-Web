<!DOCTYPE html>
<html>

<head>
  <title>Q-pon</title>

  <!-- Viewport mobile tag for sensible mobile support -->
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

  <%/* If you want to discourage search engines from indexing this site, uncomment the following line: */%>
  <%/* <meta name="robots" content="noindex"> */%>




  <!--
        Stylesheets and Preprocessors
        ==============================

  
    <!--STYLES-->
    <link rel="stylesheet" href="/styles/importer.css">
    <!--STYLES END-->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.0/css/bulma.min.css">
</head>

<body>
  <nav class="navbar" role="navigation" aria-label="main navigation">
    <div class="navbar-brand">
      <a class="navbar-item" href="https://bulma.io">
        <img
          src="data:image/png;base64,"
          width="112" height="28">
      </a>

      <a role="button" class="navbar-burger burger" aria-label="menu" aria-expanded="false"
        data-target="navbarBasicExample">
        <span aria-hidden="true"></span>
        <span aria-hidden="true"></span>
        <span aria-hidden="true"></span>
      </a>
    </div>

    <div id="navbarBasicExample" class="navbar-menu">
      <div class="navbar-start">
        <a href="http://localhost:1337/qpon/home" class="navbar-item">
          Home
        </a>
        <a href="http://localhost:1337/qpon/search?" class=" navbar-item">
          Search
        </a>
        <% if (req.session.userrole == 'admin') { %>

        <a href="http://localhost:1337/qpon/create" class="navbar-item">
          Create
        </a>
        <a href="http://localhost:1337/qpon/admin" class="navbar-item">
          Admin
        </a>

        <% } %>
        <% if (req.session.userrole == 'member') { %>

        <a href="http://localhost:1337/user/redeemed"  class="navbar-item"> My Redeemed Coupons </a>

        <% } %>


      </div>
    </div>

    <div class="navbar-end">
      <div class="navbar-item">
        <% if (req.session.userrole != 'admin' && req.session.userrole != 'member') { %>

        <div class="buttons">
          <a class="button is-primary">
            <strong>Sign up</strong>
          </a>
          <a class="button is-light" href='http://localhost:1337/user/login'>
            Log in
          </a>
        </div>

        <% } %>

        <% if (req.session.userrole == 'member'||req.session.userrole =='admin') { %>

        <div> Hi, <%= req.session.username %>  </div>

        <form action="/user/logout" method="POST" >
        <div class="buttons">

          <button type="submit" class="button is-light" onclick="logout(this)">
            Log out
          </button>
        </div>
        </form>
        
        

        <% } %>


      </div>
    </div>
  </nav>

  <section class="section">
    <div class="container">
      <div style="font-size: 30px;">Q-Pon</div>
      <div style="font-size:16px">Redeem your coins for dinning coupons.</div>
    </div>
  </section>

  <section class="section">
    <div class="container">
      <div class="column">

        <%- body %>

      </div>
    </div>
  </section>







  <!--SCRIPTS-->
  <script src="/dependencies/sails.io.js"></script>
  <!--SCRIPTS END-->
  <script defer src="https://use.fontawesome.com/releases/v5.3.1/js/all.js"></script>
</body>

</html>